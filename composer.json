{
    "name": "yosymfony/spress",
    "description": "Static site generator with blogs support",
    "keywords": ["cms","blog","web","markdown","twig","silex","html"],
    "license": "MIT",
    "homepage": "http://spress.yosymfony.com",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "twig/twig": ">=1.8,<2.0-dev",
        "pimple/pimple": "~2.1",
        "symfony/yaml": "~2.4",
        "symfony/console": "~2.4",
        "symfony/finder": "~2.4",
        "symfony/filesystem": "~2.4",
        "symfony/event-dispatcher": "~2.4",
        "michelf/php-markdown": "1.4.*@dev",
        "composer/composer": "1.*@dev",
        "dflydev/embedded-composer-core": "1.0.*@dev",
        "dflydev/apache-mime-types": "1.0.*@dev",
        "yosymfony/config-loader": "~1.0-dev",
        "yosymfony/httpserver": "~1.0-dev",
        "yosymfony/resource-watcher": "~1.0-dev",
        "yosymfony/spress-theme-spresso": "1.0.*@dev"
    },
    "replace": {
        "yosymfony/spress-core": "self.version"
    },
    "bin": [
        "bin/spress"
    ],
    "autoload": {
        "psr-4": {
            "Yosymfony\\Spress\\": "src/Yosymfony/Spress/",
            "Yosymfony\\Spress\\Plugin\\": "src/Yosymfony/Spress/Core/Plugin/BackwardsCompatibility/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    }
}
